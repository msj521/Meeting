@charset "UTF-8";
@font-face {
    font-family: PingFangSC-Regular;
    font-weight: normal;
    font-style: normal;
}

html,
body,
div,
span,
applet,
object,
iframe,
h1,
h2,
h3,
h4,
h5,
h6,
p,
blockquote,
pre,
a,
abbr,
acronym,
address,
big,
cite,
code,
del,
dfn,
em,
img,
input,
button,
ins,
kbd,
q,
s,
samp,
small,
strike,
strong,
sub,
sup,
tt,
var,
b,
u,
i,
center,
dl,
dt,
dd,
ol,
ul,
li,
fieldset,
form,
label,
legend,
table,
caption,
tbody,
tfoot,
thead,
tr,
th,
td,
article,
aside,
canvas,
details,
embed,
figure,
figcaption,
footer,
header,
hgroup,
menu,
nav,
output,
ruby,
section,
summary,
time,
mark,
audio,
video {
    margin: 0;
    padding: 0;
    border: 0;
    font-size: 100%;
    font: inherit;
    font-weight: normal;
    vertical-align: baseline;
}

article,
aside,
details,
figcaption,
figure,
footer,
header,
hgroup,
menu,
nav,
section {
    display: block;
}

ol,
ul,
li {
    list-style: none;
}

blockquote,
q {
    quotes: none;
}

blockquote:before,
blockquote:after,
q:before,
q:after {
    content: '';
    content: none;
}

table {
    border-collapse: collapse;
    border-spacing: 0;
}

a {
    color: #7e8c8d;
    text-decoration: none;
    -webkit-backface-visibility: hidden;
}

::-webkit-scrollbar {
    width: .05rem;
    height: .05rem;
}

::-webkit-scrollbar-track-piece {
    background-color: rgba(0, 0, 0, 0.2);
    -webkit-border-radius: .06rem;
}

::-webkit-scrollbar-thumb:vertical {
    height: .05rem;
    background-color: rgba(125, 125, 125, 0.7);
    -webkit-border-radius: .06rem;
}

::-webkit-scrollbar-thumb:horizontal {
    width: .05rem;
    background-color: rgba(125, 125, 125, 0.7);
    -webkit-border-radius: .06rem;
}

html,
body {
    width: 100%;
    height: 100%;
    font-family: "Microsoft YaHei";
}

body {
    line-height: 1;
    -webkit-text-size-adjust: none;
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}

html {
    overflow-y: scroll;
}

.dn {
    display: none;
}

.clear_float::after {
    content: "";
    display: block;
    height: 100%;
    clear: both;
}

input {
    border: .01rem solid #ccc;
    border-radius: .05rem;
    padding: .05rem .1rem;
    font-size: .18rem;
    color: #666;
    box-sizing: border-box;
}

html {
    font-size: 26.6667vw;
}

body {
    font-size: .18rem;
    font-family: '微软雅黑';
    position: relative;
    padding-top: .44rem;
    box-sizing: border-box;
}

.red {
    color: red;
}

header {
    font: 400 .24rem/.44rem '微软雅黑';
    text-align: center;
    height: .44rem;
    background: #0C7753;
    color: #fff;
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
}

main {
    padding: .2rem .15rem .2rem;
    height: 100%;
    box-sizing: border-box;
    overflow-y: scroll;
    overflow-x: hidden;
}

main>h3 {
    line-height: .28rem;
    text-align: center;
    color: red;
    font-size: .18rem;
    padding-bottom: .1rem;
}

main .form_box {
    padding: .2rem 0;
    line-height: .24rem;
    font-size: .16rem;
}

.form_box ul {
    padding: 0 .1rem;
}

.form_box ul>li dl dt b {
    vertical-align: middle;
}

.form_box ul>li dl dd {
    height: .36rem;
}

.form_box ul>li dl dd.radio_dd {
    height: auto;
    border: .01rem solid #ccc;
    border-radius: .05rem;
}

.form_box ul>li dl dd.radio_dd>label {
    border-bottom: .01rem solid #ccc;
}

.form_box ul>li dl dd.radio_dd>label:last-child {
    border: 0;
}

.form_box ul>li dl dd>label {
    width: 100%;
    display: block;
    height: .36rem;
    line-height: .36rem;
}

.form_box ul>li dl dd>label input {
    height: .2rem;
    width: auto;
    height: auto;
    margin-left: .1rem;
}

.form_box ul>li dl dd input,
.form_box ul>li dl dd>select {
    width: 100%;
    height: 100%;
    outline: none;
    border-radius: .05rem;
}

.form_box .submitBtn {
    padding: .2rem .1rem;
    line-height: .4rem;
}

.form_box .submitBtn button {
    width: 100%;
    height: 100%;
    background: #0C7753;
    color: #fff;
    border-radius: .1rem;
    outline: none;
}