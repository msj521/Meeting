{include file="public/header" /}
<div class="conter980" style="font-size:18px;">
    <h3>SIFIC 2019卓越感控追梦之星</h3>
    <h4 style="text-align:center;"><a href="/excellent_introduce" target="_blank" style="font-size: 20px;color: blue;">卓越感控追梦之星</a></h4>
    <div class="apply_term apply_term_1 clear">
        <div style="float:left;">姓名：<input type="text" style="width:80px;" class="apply_name"></div>
        <div style="float:left;margin-left: 70px;">性别：<select class="form-control apply_sex" style="width: 53%;display: inline-block;background:#F8F8F7;"><option>男</option><option>女</option></select></div>
        <div style="float:left;margin-left: 70px;"><span>SIFIC会员名:</span><input type="text" class="apply_sific_name"></div>
        <div style="float:left;margin-left: 70px;"><span>会员等级:</span><input type="text" class="apply_member_level"></div>
    </div>
    <div class="apply_term apply_term_2 clear">
        <div class="abs_inputlist hideinput" style="width:auto;float:left;">
            <div class="abs_input_lf">
                省份/城市<span></span>
            </div>
            <div class="abs_input_rg">
                <select id="province_id" class="form-control" data-live-search="true" style="width: 47%; float: left;background:#F8F8F7;">
                        <option value="0">请选择</option>
                      </select>
                <select id="city_id" class="form-control" data-live-search="true" style="width: 47%; float: left; margin-left: 6%;background:#F8F8F7;">
                        <option value="0">请选择</option>
                      </select>
                <div class="annual_tips_ys" id="ss_tilps" style="margin-top: 10px;"></div>
            </div>
        </div>
        <div style="float:left;line-height: 50px;margin-left: 50px;"><span>部门:</span><input type="text" style="width: 130px;" class="apply_department"></div>
        <div style="float:left;line-height: 50px;margin-left: 15px;"><span>邮编:</span><input type="text" style="width: 80px" class="apply_postal_code"></div>
    </div>
    <div class="apply_term apply_term_3 clear">
        <div style="float:left;">
            <div class="abs_input_lf">
                入职年月<span></span>
            </div>
            <div class="abs_input_rg" style="width:auto;">
                <input type="text" class="Myclass_timeselect clssdatetime0 apply_entry_date" name="" id="entry_day" value="" style="width:100px;height:34px;" />
                <div class="annual_tips_ys"></div>
            </div>
        </div>
        <div style="float:left;">
            <div class="abs_input_lf">
                注册时间<span></span>
            </div>
            <div class="abs_input_rg" style="width:auto;">
