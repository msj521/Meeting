body,
html {
    width: 100%;
    height: 100%;
}

body {
    background: #fff;
    font-family: "微软雅黑", Arial, Helvetica, sans-serif;
    font-size: 100%;
}

input {
    outline: none;
}

h1,
h2,
h3 {
    padding: 0px;
    margin: 0px;
}

ul,
li {
    list-style-type: none;
    padding: 0px;
    margin: 0px;
}

a:hover,
a:link,
a:active,
a:after {
    text-decoration: none;
}

.ellipsis {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.margintop20 {
    margin-top: 20px;
}

.paddingtop30 {
    padding-top: 30px;
}

.paddingbottom30 {
    padding-bottom: 30px;
}


/*top*/

.navbarfixed-top {
    height: 60px;
    background: #fff;
    -webkit-box-shadow: 0 0px 6px rgba(221, 221, 221, 1);
    -moz-box-shadow: 0 0px 6px rgba(221, 221, 221, 1);
    box-shadow: 0 0px 6px rgba(221, 221, 221, 1);
}

.navct,
.submenulist,
.conter960 {
    width: 1280px !important;
    margin: 0px auto;
}

.conter980 {
    width: 980px;
    margin: 0 auto;
}

.conter1000 {
    width: 1000px;
    margin: 0 auto;
}

.conter960::after {
    content: "";
    display: block;
    clear: both;
}

.uer_conter1000 {
    width: 1000px;
    margin: 0 auto;
}

.annual_conter960 {
    width: 960px;
    margin: 0 auto;
}

.logo {
    margin-top: 10px;
    float: left;
}

.logo img {}

.navlist {
    float: left;
    margin-left: 46px;
}

.navright {
    float: right;
    width: 330px;
}

.navcts {
    width: 100%;
    background: #f0f0f0;
    position: fixed;
    z-index: 99;
    top: 80px;
    left: 0;
    right: 0;
    display: none;
}

#navigation {}

#navigation ul li.noenli {
    float: left;
    width: 52px;
    line-height: 48px;
    position: relative;
    z-index: 100;
    margin-right: 30px;
}

#navigation ul li a.active {
    color: #000;
    border-bottom: 2px solid #000;
}

#navigation ul li a {
    display: block;
    line-height: 48px;
    text-align: center;
    color: #676767;
    font-size: 18px;
    font-weight: normal;
}

#navigation ul li a:hover {
    color: #000;
    border-bottom: 2px solid #000;
}

#navigation ul li ul {
    width: 960px;
    margin: 0px auto;
}

#navigation ul li.noenli ul li {
    float: left;
    margin-left: 56px;
    height: 60px;
    line-height: 60px;
}

#navigation ul li.noenli ul li a {
    color: #555;
    font-size: 16px;
}

#navigation ul li.noenli ul li a:hover {
    color: #416cac;
}


/*登录*/

.ctpageloginlist {
