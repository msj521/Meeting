<script>
		var download = "api/convention/exhibitor_list";
		function mtdownload(data){
			if(data.data.exhibitor_list==undefined||data.data.exhibitor_list==""){
				return false;
			}
			for(var i=0; i<data.data.exhibitor_list.length;i++){
				if(data.data.exhibitor_list[i].exhibitor_type==1){
					$('<a href="'+data.data.exhibitor_list[i].web_url+'" target="_blank">'+'<img src="'+data.data.exhibitor_list[i].image_url+'">'+'</a>').appendTo("#tej");
				}else if(data.data.exhibitor_list[i].exhibitor_type==2){
					$('<a href="'+data.data.exhibitor_list[i].web_url+'" target="_blank">'+'<img src="'+data.data.exhibitor_list[i].image_url+'">'+'</a>').appendTo("#Aji");
				}else if(data.data.exhibitor_list[i].exhibitor_type==3){
					$('<a href="'+data.data.exhibitor_list[i].web_url+'" target="_blank">'+'<img src="'+data.data.exhibitor_list[i].image_url+'">'+'</a>').appendTo("#Bji");
				}else{
					$('<a href="'+data.data.exhibitor_list[i].web_url+'" target="_blank">'+'<img src="'+data.data.exhibitor_list[i].image_url+'">'+'</a>').appendTo("#Cji");
				}
			}
			
		}
		var data = {"convention_id":convention_id};
   		ajax_all_Filed("true", "true", "POST", download, "json", data, mtdownload);//调用函数


</script>
